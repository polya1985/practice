   "source": [
    "L=list(map(int,input().split()))\n",
    "n=len(L)\n",
    "def mid(L):\n",
    "    L.sort()\n",
    "    L.remove(max(L))\n",
    "    L.remove(min(L))\n",
    "    return L[0]\n",
    "\n",
    "for i in range(3,n):\n",
    "    p=abs(L[i]-L[i-1])\n",
    "    if p>5:\n",
    "        LL=[L[i-3],L[i-2],L[i-1]]\n",
    "        L[i]=mid(LL)\n",
    "for i in range(n):\n",
    "    print(L[i],end=' ')"
   ]
